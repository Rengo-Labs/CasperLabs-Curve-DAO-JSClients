{
  "name": "MULTICALL-JsClients",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deployMulticall": "ts-node MULTICALL/scripts/deployMulticall.ts",
    "aggregate": "ts-node MULTICALL/scripts/aggregate.ts",
    "getCurrentBlockTimestampsessioncode": "ts-node MULTICALL/scripts/getCurrentBlockTimestampsessioncode.ts"
  },
  "Script deployMulticall comments": {
    "Description" : "use it to deploy Multicall",
    "Syntax" : "npm run deployMulticall",
    "Example" : "npm run deployMulticall"
  },
  "Script aggregate comments": {
    "Description" : "use it to call aggregate",
    "Syntax" : "npm run aggregate <targetAddr> <functionName> <functionArgsName> <functionArgsValue>",
    "Example" : "npm run aggregate 4af852da890104582a535d90ea369a5b9a1bb1fcc19874fdaf9f8f6dc4abedae name name 4af852da890104582a535d90ea369a5b9a1bb1fcc19874fdaf9f8f6dc4abedae"
  },
  "Script getCurrentBlockTimestampsessioncode comments": {
    "Description" : "use it to get Current Block Timestamp through sessioncode",
    "Syntax" : "npm run getCurrentBlockTimestampsessioncode",
    "Example" : "npm run getCurrentBlockTimestampsessioncode"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "blakejs": "^1.1.1",
    "casper-js-sdk": "^2.7.5",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "ts-results": "^3.3.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3"
  }
}
